{
    "name": "ngj-frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "emotion": "^8.0.9",
        "mobx": "^3.3.1",
        "mobx-react": "^4.3.4",
        "mobx-router": "0.0.7",
        "moment": "^2.19.2",
        "node-sass-chokidar": "0.0.3",
        "npm-run-all": "^4.1.1",
        "react": "^16.0.0",
        "react-app-rewire-emotion": "^3.0.0",
        "react-app-rewire-mobx": "^1.0.7",
        "react-app-rewire-preact": "^1.0.1",
        "react-app-rewire-sass": "^1.2.3",
        "react-app-rewired": "^1.3.4",
        "react-autocomplete": "^1.7.2",
        "react-dom": "^16.0.0",
        "react-emotion": "^8.0.9",
        "react-scripts": "1.0.16",
        "react-select-plus": "^1.2.0",
        "react-social-login": "^3.4.0",
        "react-table": "^6.7.6",
        "semantic-ui-css": "^2.2.12",
        "semantic-ui-react": "^0.77.1",
        "superagent": "^3.8.0",
        "superagent-promise": "^1.1.0"
    },
    "scripts": {
        "start-js": "react-app-rewired start",
        "start": "npm-run-all -p watch-css start-js",
        "build-js": "react-app-rewired build",
        "build": "npm-run-all build-css build-js",
        "eject": "react-scripts eject",
        "test": "react-app-rewired test --env=jsdom",
        "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
    },
    "devDependencies": {
        "babel-plugin-emotion": "^8.0.9",
        "node-sass": "^4.5.3"
    }
}
